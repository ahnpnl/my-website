{
  "name": "my-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.1",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.7.0",
    "redux-promise-middleware": "^4.3.0"
  },
  "devDependencies": {
    "autoprefixer": "9.7.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "cssnano": "^3.10.0",
    "dotenv": "4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "flow-bin": "^0.48.0",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.1.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lost": "^8.0.0",
    "node-sass": "^4.5.3",
    "normalize-scss": "^7.0.0",
    "object-assign": "4.1.1",
    "postcss": "^7.0.21",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "postcss-pxtorem": "^4.0.1",
    "promise": "7.1.1",
    "react-dev-utils": "^3.1.2",
    "react-error-overlay": "^1.0.7",
    "react-modal": "^2.2.2",
    "react-popper": "^0.7.0",
    "react-styleguidist": "^5.4.4",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.0",
    "sassdoc": "^2.3.0",
    "sinon": "^11.1.1",
    "style-loader": "0.17.0",
    "svg-sprite-loader": "^3.0.5",
    "svgo-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "styleguide": "NODE_ENV=dev styleguidist server",
    "styleguide:build": "NODE_ENV=dev styleguidist build",
    "coverage": "yarn run test -- --coverage",
    "lint": "eslint src",
    "flow:check": "flow check",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.spec.js' -t html -o 'coverage/flow-report'",
    "precommit": "yarn lint",
    "prepush": "yarn lint",
    "sassdoc": "sassdoc 'src/assets/sass/*'",
    "css:watch": "node-sass src/assets/sass/main.scss sass/index.css -w"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/{index.js,reducers.js,store.js,raven.js,console.js}",
      "!src/locales/**/*.js",
      "!src/_mocks_/*.js",
      "!src/assets/*.*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
